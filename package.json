{
    "name": "vue-snippet",
    "displayName": "vue-snippet",
    "description": "This is a vue snippet extenssion, include vue, vue components, eg viewdesign",
    "version": "0.0.3",
    "logo":"images/logo.png",
    "publisher": "vue-snippet",
    "engines": {
        "vscode": "^1.14.0"
    },
    "keywords": ["vue", "viewdesign", "snippet"],
    "repository": {
        "type": "git",
        "url": "https://github.com/byself/vue-components-snippets"
      },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
